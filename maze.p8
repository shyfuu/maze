--split 2d array
function split2d(s)
	local arr=split(s,"|",false)
	for k,v in pairs(arr) do
		arr[k]=split(v)
	end
	return arr
end

--return tile number (0 to 255)
function mapget(self,ox,oy)
	local ox,oy=ox or 0,oy or 0
	return mget((self.x+ox)\8,(self.y+oy)\8)
end

--return tile flag (0 to 7)
function getflag(self,ox,oy,flag)
	return fget(mapget(self,ox,oy),flag)
end

--return tile x/y in pixel
function get_tile_coord(self,ox,oy)
	return ((self.x+ox)\8)*8,((self.y+oy)\8)*8
end

function snap_2_grid_x(self)
	return ((self.x+self.w/2)\8)*8
end

function snap_2_grid_y(self)
	return ((self.y+self.h/2)\8)*8
end



