function collide_side(self)
	local offset=self.w-1
	for i=2,self.w-2 do
		if self.dx>0 then
			if getflag(self,offset,i,0) then
				self.dx=0
				self.x=((self.x\8)*8)+1
				return true
			end
		elseif self.dx<0 then
			if getflag(self,0,i,0) then
				self.dx=0
				self.x=((self.x\8)*8)+offset
				return true
			end
		end
	end
	return false
end

function collide_floor(self)
	if self.dy<0 then 
		return false
	end
	local landed=false
	
	for i=2,self.w-2 do
		local tile=mapget(self,i,self.h+2)
		if fget(tile,0) or (fget(tile,1) and self.dy>=0) then
			if fget(tile,2) and self.state=="ladd" then
				//ladder wall collision 
				//nothing
			else
				self.dy=0
				self.y=(flr((self.y+self.h+2)/8)*8)-self.h
				self.grounded=true
				self.drop=false
				self.airtime=0
				landed=true
				if self.oldstate=="jump" then
					sfx(fx["land"])
					self.oldstate="idle"
				end
			end
		end
	end
	return landed
end

function collide_roof(self)
	for i=2,self.w-2 do
		if getflag(self,i,nil,0) then
			self.dy=0
			self.y=flr((self.y)/8)*8+8
		end
	end
end

