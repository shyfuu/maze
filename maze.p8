--make the player
function m_player(x,y)
	local p={
		x=x*8,
		y=y*8,
		dx=0,
		dy=0,
		w=8,
		h=8,
		
		max_dx=.6,
		max_dy=2.6,
		
		vel=2.6, 
		acc=.6,
		
		grav=.3,
		dcc=.3,
		air_dcc=.5,
		
		‚ùé_button={
			is_pressed=false,
			is_down=false,
			upd=function(self)
				self.is_pressed=false
				if btn(‚ùé) then
					if not self.is_down then
						self.is_pressed=true
					end
					self.is_down=true
				else
					self.is_down=false
					self.is_pressed=false
				end
			end
		},
		
		üÖæÔ∏è_button={
			is_pressed=false,
			is_down=false,
			upd=function(self)
				self.is_pressed=false
				if btn(üÖæÔ∏è) then
					if not self.is_down then
						self.is_pressed=true
					end
					self.is_down=true
				else
					self.is_down=false
					self.is_pressed=false
				end
			end
		},
		
		--player states
		grounded=false,
		drop=false,
		state="idle",
		oldstate="idle",
		--time since not grounded
		airtime=0,
		
		--animation definitions
		anims={
			["idle"]={
				ticks=1,
				frames={1}
			},
			["run"]={
				ticks=5,
				frames={2,3,4,5}
			},
			["jump"]={
				ticks=1,
				frames={6}
			},
			["fall"]={
				ticks=1,
				frames={2}
			},
			["ladd"]={
				ticks=5,
				frames={7,8}
			},
			["hang"]={
				ticks=1,
				frames={9}
			},
			["push"]={
				ticks=7,
				frames={10,11}
			},
			["roof"]={
				ticks=7,
				frames={12,13,14,15}
			}
		},
		
		curanim="run",
		curframe=1,
		animtick=0,
		flipx=false,
		
		--request new animation to play.
		set_anim=function(self,anim)
			if (anim==self.curanim) return--early out.
			local a=self.anims[anim]
			self.animtick=a.ticks--ticks count down.
			self.curanim=anim
			self.curframe=1
		end,
		
		upd_animtick=function(self)
			if self.state=="ladd" then
				--tick is following player.dy
				self.animtick-=abs(self.dy)*2
			else
				self.animtick-=1
			end
			
			if self.animtick<0 then
				self.curframe+=1
				local a=self.anims[self.curanim]
				self.animtick=a.ticks--reset timer
				if self.curframe>#a.frames then
					self.curframe=1--loop
				end
			end
		end,
		
		upd_flip=function(self,br,bl)
			if br then
				self.flipx=false
			elseif bl then
				self.flipx=true
			end
		end,
		
		upd=function(self)
			
			--track button presses
			local bl=btn(0) --left
			local br=btn(1) --right
			local bu=btn(2) --up
			local bd=btn(3) --down
			
			--ladders check
			if self.state=="ladd" then
				bl,br=false,false
			end
			--move left/right
			if bl then
				self.dx-=self.acc
				br=false
			elseif br then
				self.dx+=self.acc
			else
				if self.grounded then
					self.dx*=self.dcc
				else
					self.dx*=self.air_dcc
				end
			end
			
			--move x
			self.dx=mid(-self.max_dx,self.dx,self.max_dx)
			self.x+=self.dx
			collide_side(self)
			
			--jump 
			self.‚ùé_button:upd()
			if self.‚ùé_button.is_pressed then
				local on_ground=(self.grounded or self.airtime<5)
				if self.state=="ladd" then
					self.state="jump"
					self.oldstate="ladd"
					on_ground=true
				end
				if on_ground then
					sfx(fx[jump])
					self.oldstate="jump"
					self.dy=-self.vel
				end
			end
			
			--drop
			self.üÖæÔ∏è_button:upd()
			if self.üÖæÔ∏è_button.is_pressed 
			and not bu and not bd then
				if self.state=="ladd" 
				or self.state=="hang"
				or self.state=="roof" then
					self.state="fall"
					self.drop=true
				end
			end
			
			--ladders
			if bu or bd or self.dy>0 
			and not self.drop then
				grab_ladder(self)
			end
			
			--move y
			if self.state!="ladd" then
				self.dy+=self.grav
			else
				--ladders
				if bu then
					if is_ladder(self)=="up" then
						self.dy=-self.acc/2
					else
						self.state="idle"
					end
				elseif bd then
					self.dy=self.acc/2
				else
					self.dy=0
				end
			end
			
			self.dy=mid(-self.max_dy,self.dy,self.max_dy)
			self.y+=self.dy
			
			--floor collision
			if not collide_floor(self) then
				self.grounded=false
				if self.state!="ladd" then
					self.airtime+=1
				end
			end
			
			--roof
			collide_roof(self)
			
			--player states
			if self.grounded then
				if bl or br then
					self.state="run"
				else
					self.state="idle"
				end
			else
				if self.state!="ladd" 
				and self.state!="hang" 
				and self.state!="roof" then
					if self.dy<0 then
						self.state="jump"
					elseif self.dy>0 then
						self.state="fall"
					end
				end
			end
			
			--x limit
			self.x=mid(0,self.x,56)
			--y limit (temporary)
			if self.y>128 then
				_init()
			end
			
			--flip x axis
			self:upd_flip(br,bl)
			--animations
			self:upd_animtick()
			self:set_anim(self.state)
			
			---------debug stuff--------
			debug[1]="state:"..self.state
			----------------------------
		end,
		
		drw=function(self)
			local a=self.anims[self.curanim]
			local frame=a.frames[self.curframe]
			spr(frame,self.x,self.y,self.w/8,self.h/8,self.flipx,false)
			--hitbox
			--rect(self.x+1,self.y,self.x+self.w-2,self.y+self.h-1,8)
		end
	}
	return p
end

function is_ladder(self)
	local w=self.w/2
	local flag=2
	if getflag(self,w,self.h-1,flag) then
		return "up" 
	elseif getflag(self,w,self.h,flag) then
		return "down"
	end
end

function grab_ladder(self)
	if is_ladder(self) then
		local x,y=get_tile_coord(self,self.w/2,0)
		for i=-2,2 do 
			if flr(self.x)==x+i then
				self.dx,self.dy=0,0
				self.x=snap_2_grid_x(self)
				self.state="ladd"
				self.oldstate="ladd"
				self.airtime=0
			end
		end
	else
		self.state="idle"
	end
end


